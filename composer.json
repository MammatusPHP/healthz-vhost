{
    "name": "mammatus/http-server",
    "type": "composer-plugin",
    "description": "\ud83d\udc19 Multi vhost HTTP Server",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "cakephp/collection": "^3.6",
        "chimera/bus-tactician": "^0.3.0",
        "chimera/mapping": "^0.3.0",
        "chimera/routing": "^0.3.4",
        "chimera/routing-expressive": "^0.3.0",
        "composer-plugin-api": "^1.1",
        "doctrine/annotations": "^1.6",
        "joshdifabio/composed": "^1.0",
        "laminas/laminas-diactoros": "^2.2",
        "lcobucci/content-negotiation-middleware": "^2.2",
        "lcobucci/jwt": "^3.3",
        "mammatus/app": "dev-master",
        "mammatus/http-server-annotations": "dev-master",
        "mammatus/http-server-contracts": "dev-master",
        "mammatus/http-server-webroot": "dev-master",
        "mammatus/life-cycle-events": "dev-master",
        "mammatus/metrics-prometheus": "dev-master",
        "mezzio/mezzio-helpers": "^5.3",
        "mezzio/mezzio-router": "^3.1",
        "middlewares/access-log": "^1.0",
        "middlewares/cache": "^1.0",
        "middlewares/client-ip": "^1.0",
        "middlewares/filesystem": "^1.0",
        "middlewares/https": "^1.1",
        "middlewares/response-time": "^1.0",
        "middlewares/uuid": "^1.0",
        "nikic/fast-route": "^1.2",
        "opis/closure": "^3.5",
        "php-di-definitions/react-event-loop-inspected": "dev-master",
        "react-inspector/http-middleware": "^1.0",
        "react-parallel/event-loop": "^1.0",
        "react-parallel/logger": "dev-master",
        "react-parallel/psr-15-adapter": "dev-master",
        "react-parallel/streams": "dev-master",
        "react-parallel/worker-pool": "dev-master",
        "react/http": "^0.8.0",
        "roave/better-reflection": "^4.0",
        "thecodingmachine/safe": "^1.0",
        "thruway/middleware": "^0.0.2",
        "voryx/thruway": "^0.5.3",
        "wyrihaximus/broadcast": "dev-master",
        "wyrihaximus/broadcast-contracts": "^1.0",
        "wyrihaximus/composer-update-bin-autoload-path": "dev-master",
        "wyrihaximus/doctrine-annotation-autoloader": "^1.0",
        "wyrihaximus/get-in-packages-composer.jason": "^1.0",
        "wyrihaximus/iterator-or-array-to-array": "^1.1",
        "wyrihaximus/json-psr7": "^1.0",
        "wyrihaximus/list-classes-in-directory": "^1.3",
        "wyrihaximus/psr-3-callable-throwable-logger": "^2.0",
        "wyrihaximus/psr-3-context-logger": "^1.0",
        "wyrihaximus/react-http-middleware-resume-response-body": "^1.0",
        "wyrihaximus/react-http-middleware-rewrite": "^1.0",
        "wyrihaximus/react-http-middleware-runner": "^1.0",
        "wyrihaximus/react-http-middleware-session": "^3.0",
        "wyrihaximus/react-http-middleware-twig": "^1.0",
        "wyrihaximus/react-http-middleware-webroot-preload": "^1.0",
        "wyrihaximus/react-stream-json": "^1.2",
        "wyrihaximus/recoil-promise-coroutine-wrapper": "^1.0",
        "wyrihaximus/recoil-queue-caller-pool": "^1.2",
        "wyrihaximus/simple-twig": "^1.1",
        "wyrihaximus/string-get-in": "^1.0",
        "wyrihaximus/to-child-process-or-not-to-child-process": "^1.0",
        "wyrihaximus/to-coroutine-or-not-to-coroutine": "^1.0",
        "wyrihaximus/to-thread-or-not-to-thread": "dev-master",
        "zendframework/zend-diactoros": "^2.1"
    },
    "require-dev": {
        "monolog/monolog": "^2.0",
        "wyrihaximus/async-test-utilities": "^2.1"
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    },
    "extra": {
        "class": "Mammatus\\Http\\Server\\Composer\\Installer",
        "mammatus": {
            "http": {
                "server": {
                    "has-vhosts": true
                }
            }
        },
        "php-di-definitions": {
            "di": [
                "etc/di/*.php"
            ]
        },
        "wyrihaximus": {
            "broadcast": {
                "has-listeners": true
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Mammatus\\Http\\Server\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mammatus\\Tests\\Http\\Server\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "psr-15-adapter-master"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ],
        "pre-autoload-dump": [
            "Mammatus\\Http\\Server\\Composer\\Installer::findVhosts"
        ],
        "generate-config": [
            "Mammatus\\Http\\Server\\Composer\\Installer::findVhosts"
        ]
    }
}
